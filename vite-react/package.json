  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "serve": "vite preview --host 0.0.0.0",
    "clean": "rm -rf dist",
    "lint": "concurrently 'yarn:lint:*'",
    "lint:eslint": "eslint '**/*.{ts,js,tsx,jsx}'",
    "lint:prettier": "prettier --check '**/*.{ts,js,json,md,yml}'",
    "lint:styles": "stylelint --ignore-path .eslintignore '**/*.scss'",
    "fix": "concurrently 'yarn:fix:*'",
    "fix:eslint": "eslint --fix '**/*.{ts,js}'",
    "fix:prettier": "prettier --write '**/*.{ts,js,tsx,jsx,json,md,yml}'",
    "fix:styles": "stylelint --fix --ignore-path .eslintignore '**/*scss'",
    "test": "jest --watchAll --verbose",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "@reach/router": "^1.3.4",
    "prop-types": "^15.7.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "sanitize.css": "^13.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.11",
    "@types/reach__router": "^1.3.9",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.11",
