{
  "name": "snowpack-vue-template",
  "version": "0.1.0",
  "description": "TODO",
  "author": "Matt Cook <matt@sciencefidelity.co.uk>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "snowpack dev",
    "build": "snowpack build",
    "serve": "serve build -l 8080",
    "clean": "rm -rf build && yarn snowpack --reload",
    "lint": "concurrently 'yarn:lint:*'",
    "lint:js": "eslint --ext .js,.ts,.vue .",
    "lint:format": "prettier --check '**/*.{ts,js,json,md,yml,vue}'",
    "lint:styles": "stylelint '**/*.{vue,scss}' --ignore-path .eslintignore",
    "fix": "concurrently 'yarn:fix:*'",
    "fix:js": "eslint --ext .js,.ts.vue --fix .",
    "fix:format": "prettier --write '**/*.{ts,js,json,md,yml,vue}'",
    "fix:styles": "stylelint --fix '**/*.{vue,scss}' --ignore-path .eslintignore",
    "test": "echo \"This template does not include a test runner.\" && exit 1"
  },
  "dependencies": {
    "sanitize.css": "^13.0.0",
    "vue": "next",
    "vue-class-component": "next",
    "vue-property-decorator": "next",
    "vue-router": "next"
  },
  "devDependencies": {
    "@snowpack/plugin-dotenv": "^2.2.0",
    "@snowpack/plugin-sass": "^1.4.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@snowpack/plugin-vue": "^2.6.2",
    "@types/node": "^17.0.6",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "concurrently": "^6.5.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "postcss": "^8.4.5",
    "postcss-html": "^1.3.0",
    "postcss-scss": "^4.0.2",
    "prettier": "^2.5.1",
    "rollup": "^2.62.0",
    "serve": "^13.0.2",
    "snowpack": "^3.8.8",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "typescript": "^4.5.4",
    "vue-eslint-parser": "^8.0.1",
    "vue-tsc": "^0.30.1"
  }
}
