{
  "name": "snowpack-vue-template",
  "version": "0.1.0",
  "description": "TODO",
  "license": "MIT",
  "author": "Matt Cook <matt@sciencefidelity.co.uk>",
  "private": true,
  "scripts": {
    "dev": "snowpack dev",
    "build": "snowpack build",
    "serve": "serve build -l 8080",
    "clean": "rm -rf build && yarn snowpack --reload",
    "lint": "concurrently 'yarn:lint:*'",
    "lint:js": "eslint --ext .js,.ts,.vue .",
    "lint:prettier": "prettier --check '**/*.{ts,js,json,md,yml,vue}'",
    "lint:styles": "stylelint '**/*.{scss,vue}' --ignore-path .eslintignore",
    "fix": "yarn fix:js && yarn fix:style ",
    "fix:js": "eslint --ext --fix .js,.ts.vue .",
    "fix:prettier": "prettier --write '**/*.{ts,js,json,md,yml,vue}'",
    "fix:styles": "stylelint --fix '**/*.{scss,vue}' --ignore-path .eslintignore",
    "test": "echo \"This template does not include a test runner.\" && exit 1"
  },
  "dependencies": {
    "sanitize.css": "^13.0.0",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.3"
  },
  "devDependencies": {
    "@snowpack/plugin-dotenv": "^2.2.0",
    "@snowpack/plugin-sass": "^1.4.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@snowpack/plugin-vue": "^2.6.2",
    "@types/node": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "concurrently": "^6.5.1",
    "eslint": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "prettier": "^2.5.1",
    "serve": "^13.0.2",
    "snowpack": "^3.8.8",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-processor-arbitrary-tags": "^0.1.0",
    "stylelint-scss": "^4.1.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "typescript": "^4.5.4"
  }
}
