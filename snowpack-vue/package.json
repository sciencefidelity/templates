{
  "name": "snowpack-vue-template",
  "version": "0.1.0",
  "description": "TODO",
  "author": "Matt Cook <matt@sciencefidelity.co.uk>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "snowpack dev",
    "build": "snowpack build",
    "serve": "serve build -l 8080",
    "clean": "rm -rf build && yarn snowpack --reload",
    "lint": "concurrently 'yarn:lint:*'",
    "lint:js": "eslint --ext .js,.ts,.vue .",
    "lint:format": "prettier --check '**/*.{ts,js,json,md,yml,vue}'",
    "lint:styles": "stylelint '**/*.{vue,scss}' --ignore-path .eslintignore",
    "fix": "concurrently 'yarn:fix:*'",
    "fix:js": "eslint --ext .js,.ts.vue --fix .",
    "fix:format": "prettier --write '**/*.{ts,js,json,md,yml,vue}'",
    "fix:styles": "stylelint --fix '**/*.{vue,scss}' --ignore-path .eslintignore",
    "test": "echo \"This template does not include a test runner.\" && exit 1"
  },
  "dependencies": {
    "sanitize.css": "^13.0.0",
    "vue": "^3.2.37",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@snowpack/plugin-dotenv": "^2.2.0",
    "@snowpack/plugin-sass": "^1.4.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@snowpack/plugin-vue": "^2.6.2",
    "@types/node": "^17.0.41",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "concurrently": "^7.2.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.1.0",
    "postcss": "^8.4.14",
    "postcss-html": "^1.4.1",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.6.2",
    "rollup": "^2.75.6",
    "serve": "^13.0.2",
    "snowpack": "^3.8.8",
    "stylelint": "^14.9.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.2.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "typescript": "^4.7.3",
    "vue-eslint-parser": "^9.0.2",
    "vue-tsc": "^0.37.3"
  }
