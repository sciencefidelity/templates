  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "serve": "vite preview --host",
    "clean": "echo \"Clean script not implemented\" && exit 1",
    "lint": "concurrently 'yarn:lint:*'",
    "lint:js": "eslint --ext .js,.ts,.vue .",
    "lint:prettier": "prettier --check '**/*.{ts,js,json,md,yml,vue}'",
    "lint:styles": "stylelint '**/*.{vue,scss}' --ignore-path .eslintignore",
    "fix": "concurrently 'yarn:fix:*'",
    "fix:js": "eslint --ext .js,.ts.vue --fix .",
    "fix:prettier": "prettier --write '**/*.{ts,js,json,md,yml,vue}'",
    "fix:styles": "stylelint --fix '**/*.{vue,scss}' --ignore-path .eslintignore",
    "test": "echo \"This template does not include a test runner.\" && exit 1"
  },
  "dependencies": {
    "sanitize.css": "^13.0.0",
    "vue": "^3.2.26",
    "vue-class-component": "^8.0.0-rc.1",
