  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "serve": "vite preview --host",
    "clean": "rm -rf dist",
    "lint": "concurrently 'yarn:lint:*'",
    "lint:js": "eslint --ext .js,.ts,.vue .",
    "lint:format": "prettier --check '**/*.{ts,js,json,md,yml,vue}'",
    "lint:styles": "stylelint '**/*.{vue,scss}' --ignore-path .eslintignore",
    "fix": "concurrently 'yarn:fix:*'",
    "fix:js": "eslint --ext .js,.ts,.vue --fix .",
    "fix:format": "prettier --write '**/*.{ts,js,json,md,yml,vue}'",
    "fix:styles": "stylelint --fix '**/*.{vue,scss}' --ignore-path .eslintignore",
    "test": "echo \"This template does not include a test runner.\" && exit 1"
  },
  "dependencies": {
    "sanitize.css": "^13.0.0",
    "vue": "^3.2.31",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/node": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.4.1",
    "postcss": "^8.4.6",
    "postcss-html": "^1.3.0",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.5.1",
    "sass": "^1.49.7",
    "stylelint": "^14.5.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-config-recommended-vue": "^1.3.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "typescript": "^4.5.5",
    "vite": "^2.8.1",
    "vue-eslint-parser": "^8.2.0",
    "vue-tsc": "^0.31.3"
  }
